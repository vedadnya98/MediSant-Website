<!DOCTYPE html><html><head>    <title>Medisant</title>    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">    <meta http-equiv="X-UA-Compatible" content="IE=edge">    <meta name="msapplication-tap-highlight" content="no">    <meta name="version" content="1.0.0">    <meta name="version-name" content="Defaulters me hai hum!">    <link rel='stylesheet' href='/css/materialize.css' />    <link rel='stylesheet' href='/css/main.css' />    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">    <style>        body{            background: #2196F3;            max-height: 100vh;            overflow: hidden;        }    </style></head><body><div id="login-bg">    <div class="row">        <div class="col s10 offset-s1 m8 offset-m2 l4 offset-l4">            <div class="card-panel white z-depth-4 center activator" id="login-slider">                <h5 class="center grey-text text-darken-3">MediSant Emergency Information</h5>                <div class="row">                    <form action="" class="col s12 m12 l12">                        <div class="input-field">                            <input id="pno" type="text" class="validate" name="pno" required >                            <label for="pno">Patient No</label>                        </div>                    </form>                    <div class="col s12 m12 l12" id="emergencylist"></div>                </div>            </div>        </div>    </div></div></body><script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="        crossorigin="anonymous"></script><script src="/js/materialize.js"></script><script src="/js/main.js"></script><% if (toastMessage != "") { %>    <% if (toastStatus == 'success') { %>        <script>            M.toast({html:'<%= toastMessage %>'}, 5000);        </script>    <% }else{ %>        <script>            M.toast({html:'<%= toastMessage %>'}, 5000);        </script>    <% } %><% } %><script>    $('#pno').focusout( function(event) {        event.preventDefault();        const id = $(this).val();        $.ajax({            url: '/emergencyData/'+id,            method: 'GET'            // data: {id: id}        }).done(function (res) {            if (res.success) {                f(res.data);            }            else {                $('#emergencylist').val('');            }        });    })    function f(ddata) {        for(i=0,len=ddata.emergency.length;i<len;i++){            console.log(i);            $("#emergencylist").append('<div class="row" id="row'+i+'">\n' +                '<h4>Patient Name : '+ddata['name']+'</h4>' +                '    <div class="input-field col s12 m3 l3">\n' +                '      Name : '+ddata.emergency[i]["name"]+''+                '    </div>\n' +                '    <div class="input-field col s12 m4 l4">\n' +                '          Contact : '+ddata.emergency[i]["contact"]+'<br> ' +                '    </div>\n' +                '    <div class="input-field col s12 m4 l4">\n' +                '          Relation : '+ddata.emergency[i]["relation"]+'<br> ' +                '    </div>\n' +                '</div>');        }    }</script></html>