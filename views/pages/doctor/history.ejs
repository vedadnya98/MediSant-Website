<%- include('../../layout/doctor_layout'); -%><div class="container">    <div class="card horizontal small">        <div class="card-image">            <img src="<%- patient.imageUrl%>" alt="" class="circle center center-block" width="20%" id="displayPic">        </div>        <div class="card-stacked">            <div class="card-content">                <p class="patientHistory">                    <span>Name : </span><%-  patient.name%>                </p>                <p>                    <span><i class="material-icons left">access_time</i>Last Visited : </span><%- moment(data['prescription'][data['prescription'].length-1]['updatedAt']).format('dddd, MMMM Do YYYY, h:mm')%>                </p>            </div>            <!--<div class="card-action">-->            <!--<a href="#">This is a link</a>-->            <!--</div>-->        </div>    </div>    <ul class="collapsible  popout">        <% for (i=data.prescription.length-1,len=0;i>=len;i--){ %>            <li>                <div class="collapsible-header"><i class="material-icons">access_time</i><%- moment(data['prescription'][i]['updatedAt']).format('dddd, MMMM Do YYYY, h:mm')%></div>                <div class="collapsible-body">                    <p>Findings :                        <% for(j=0,l= Object.keys(data['prescription'][i]['findings']).length;j<l;j++){ %>                           <%- data['prescription'][i]['findings'][j]['name']%>                        <%}%>                    </p>                    <p>Notes : <%- data['prescription'][i]['notes']%></p>                    <table>                        <thead>                        <tr>                            <th>Medicine</th>                            <th>When</th>                            <th>Duration</th>                        </tr>                        </thead>                        <tbody>                        <% for(j=0,l= Object.keys(data['prescription'][i]['medicine']).length;j<l;j++){ %>                            <tr>                                <td>                                    <%- data['prescription'][i]['medicine'][j]['name']%>                                </td>                                <td>                                    <%                                        let a = Object.keys(data['prescription'][i]['medicine'][j]['when']);                                        let b = a.length;                                        for(k=0;k<b;k++){                                    %>                                        <div class="chip"><%- a[k]%></div>                                    <%}%>                                </td>                                <td>                                    <%- data['prescription'][i]['medicine'][j]['duration']%>                                </td>                            </tr>                        <%}%>                    </table>                    <div class="center-block center">                        <br><br>                        <a href="/doctor/prescription/update/<%-data['_id']%>/<%-i%>" class="btn blue center-block center"><i class="material-icons left">edit</i>Update</a>                    </div>                </div>            </li>        <%} %>    </ul></div><%- include('../../layout/footer'); -%>