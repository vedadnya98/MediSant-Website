<%- include('../../layout/doctor_layout'); -%><div class="container">    <img src="" alt="" class="circle center center-block" width="20%" id="displayPic">    <form id="prescriptionForm" action="/doctor/prescription" method="POST" enctype="multipart/form-data">        <div class="row">            <div class="input-field col s12 m6 l6">                <input id="patientNO" type="text" class="validate" name="patientNO" value="<%- uid%>" required >                <label for="patientNO">Patient ID</label>            </div>            <div class="input-field col s12 m6 l6">                <input id="patientName" type="text" class="validate" name="patientName" placeholder=" " required>                <label for="patientName">Name</label>            </div>            <div class="input-field col s12">                <br>                <div class="chips chips-autocomplete" id="symptomsChip"></div>                <textarea id="symptoms" name="symptoms" class="materialize-textarea"  hidden placeholder="test"></textarea>                <label for="symptoms">Symptoms</label>            </div>            <div class="input-field col s12">                <input id="notes" type="text" class="materialize-textarea" name="notes">                <label for="notes">Notes</label>            </div>            <h5 class="subheader">Medicines</h5>            <div class="input-field col s12">                <input id="noMed" type="text" class="validate" name="noMed" value="0" hidden>            </div>        </div>        <span id="medilist">            <div class="row" id="row0">                <div class="input-field col s12 m5 l5">                    <div class="chips chips-autocomplete timeChip" name="timec" id="timeChip0"></div>                    <textarea name="time" class="materialize-textarea"  hidden placeholder="test"></textarea>                    <label for="timec">Select Timings</label>                </div>                <div class="input-field col s12 m5 l5">                    <input type="text" name="medicine0" id="medicine0" class="autocomplete medicine">                    <label for="medicine0">Medicine Name</label>                </div>                <div class="input-field col s12 m2 l2">                    <input type="text" class="datepicker" id="duration0">                    <label for="duration0">Duration</label>                </div>            </div>        </span>        <input type="text" name="medicineArray" id="medicineArray" hidden>        <div class="btn-floating btn-small waves-effect waves-light blue right" id="addNewRow"><i class="material-icons">add</i></div>        <div align="center">            <button type="submit" id="submittest" class="waves-effect waves-light btn blue z-depth-4">Confirm</button>            <button type="submit" id="submit" class="waves-effect waves-light btn blue z-depth-4 hide">Submit</button>        </div>    </form></div><%- include('../../layout/footer'); -%><script src="/js/autofill.js"></script><script src="/js/inputChips.js"></script>