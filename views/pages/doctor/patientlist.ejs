<%- include('../../layout/doctor_layout'); -%><div class="container">    <div class="row">        <% if(patientList.length == 0){ %>            <p class="center-block center"> No Patients</p>        <%}else{%>            <%for(i=0,len = patientList.length; i<len;i++){%>                <div class="col s12 m4 l4">                    <div class="card patientcard">                        <div class="card-image">                            <img src="<%- patientList[i]['imageUrl']%>" alt="patient image - <%- patientList[i]['uid']%>">                            <span class="card-title"><%- patientList[i]['name']%></span>                        </div>                        <div class="card-content">                            <p>                                <span class="grey-text">Patient No - </span><%- patientList[i]['uid']%> <br>                                <span class="grey-text">Contact No - </span><%- patientList[i]['contact']%> <br>                                <span class="grey-text">Blood Group - </span><%- patientList[i]['bloodGroup']%>                            </p>                        </div>                        <div class="card-action custBlue center" style="height: 100px;">                            <a href="history/<%- patientList[i]['uid']%>" class="white-text"><span></span>View History<i class="material-icons right">send</i></a><br><br>                            <a href="prescription/<%- patientList[i]['uid']%>" class="white-text"><span></span>Create Prescription<i class="material-icons right">send</i></a>                            <br>                        </div>                    </div>                </div>            <%}%>        <%}%>    </div></div><%- include('../../layout/footer'); -%>