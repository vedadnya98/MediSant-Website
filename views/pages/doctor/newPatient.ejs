<%- include('../../layout/doctor_layout'); -%><div class="container">    <div class="row">        <div class="col s12 m12 l12" >            <h4 class="header">Enter  patient number to add</h4>            <div class="card horizontal small">                <div class="card-image">                    <img src="" alt="" class="circle center center-block" width="20%" id="displayPic">                </div>                <div class="card-stacked">                    <div class="card-content">                        <form action="/doctor/new" method="POST" enctype="multipart/form-data">                            <div class="input-field">                                <input id="patientNO" type="text" class="validate" name="patientNO" required>                                <label for="patientNO">Patient Number</label>                            </div>                            <div class="input-field">                                <input id="patientName" type="text" class="validate" name="patientName" required>                                <label for="patientName">Name</label>                            </div>                            <input id="patientID" type="text" class="validate hiddendiv" name="patientID" required hidden>                            <input id="patientImage" type="text" class="validate hiddendiv" name="patientImage" required hidden>                            <div align="center">                                <br><br>                                <button type="submit" class="waves-effect waves-light btn blue z-depth-4">Add</button>                            </div>                        </form>                    </div>                    <!--<div class="card-action">-->                        <!--<a href="#">This is a link</a>-->                    <!--</div>-->                </div>            </div>        </div>    </div></div><%- include('../../layout/footer'); -%><script src="/js/autofill.js"></script>