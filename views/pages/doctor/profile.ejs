<%- include('../../layout/doctor_layout'); -%><div class="container">    <div id="profile-page-header" class="card">        <div class="card-image waves-effect waves-block waves-light">            <img  src="/img/user-profile-bg.jpg" alt="user background">        </div>        <figure class="card-profile-image">            <img src="<%- req.session.imageUrl %>" alt="profile image" class="circle z-depth-2 " width="100">        </figure>        <div class="card-content">            <div class="row">                <div class="col s12 m4">                    <h4 class="card-title grey-text text-darken-4"><%= user.name %></h4>                    <h5 class="medium-small">User ID : <%= user.uid %></h5>                    <p class="medium-small grey-text"><%= user.email %></p>                </div>            </div>            <div class="row">                <div class="col s12 m8 offset-m2">                    <ul class="collection" id="no-border">                        <li class="collection-item avatar z-depth-2" id="profile-content">								<span class="circle center">									<i class="material-icons small-med">phone_in_talk</i>								</span>                            <span class="title">Contact</span>                            <p>                                <%= user.contact %>                            </p>                        </li>                        <li class="collection-item avatat z-depth-2" id="profile-content">                            <span class="circle center">                                <i class="material-icons small-med">cake</i>                            </span>                            <span class="title">DOB : </span>                            <span class="timestamp"><%= moment(user.dateOfBirth).format('YYYY - MM - DD') %></span>                        </li>                        <li class="collection-item avatat z-depth-2" id="profile-content">                            <span class="circle center">                                <i class="material-icons small-med">healing</i>                            </span>                            <span class="title">Blood Group : </span><%= user.bloodGroup %>                        </li>                    </ul>                </div>            </div>        </div>    </div></div><%- include('../../layout/footer'); -%>