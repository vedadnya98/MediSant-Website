<%- include('../../layout/patient_layout'); -%><div class="container">    <form action="/patient/profile/edit" method="POST" enctype="multipart/form-data">        <div class="input-field">            <input id="name" type="text" class="validate" name="name" value="<%- user.name%>" placeholder="<%= user.name%>" disabled required >            <label for="name">Name</label>        </div>        <div class="input-field">            <input id="email" type="email" class="validate" name="email" value="<%= user.email %>" placeholder="<%= user.email %>" disabled required >            <label for="email">Email</label>        </div>        <div class="input-field">            <select name="bGroup" required>                <option value="<%- user.bloodGroup%>" disabled selected><%- user.bloodGroup%></option>                <option value="A+">A+</option>                <option value="A-">A-</option>                <option value="B+">B+</option>                <option value="B-">B-</option>                <option value="AB+">AB+</option>                <option value="AB-">AB-</option>                <option value="O+">O+</option>                <option value="O-">O-</option>            </select>        </div>        <div class="input-field">            <input placeholder="Date of Birth" type="text" class="datepicker" name="dateOfBirth" value="<%- moment(user.dateOfBirth).format('MMM DD, YYYY') %>" required>        </div>        <div class="input-field">            <input id="contact" type="number" class="validate" name="contact" value="<%- user.contact%>" required>            <label for="contact">Contact Number</label>        </div>        <%  let a ='';        for(i=0,len=user.allergies.length;i<len;i++){            a +=user.allergies[i]['name']+',';        }%>        <div class="input-field">            <input id="allergies" type="text" class="validate" name="allergies" value="<%- a%>" required>            <label for="allergies">Allergies</label>        </div>        <h4>Emergency Details:</h4>        <div class="input-field col s12">            <input id="noEmi" type="text" class="validate" name="noEmi" value="0" hidden>        </div>        <span id="emergencylist">            <% for(i=0,len=user.emergency.length;i<len;i++){%>                <div class="row" id="row<%-i%>">                    <div class="input-field col s12 m3 l3">                        <input name="relation<%-i%>" type="text" class="materialize-textarea" value="<%-user.emergency[i]['relation']%>">                        <label for="relation<%-i%>">Relation</label>                    </div>                    <div class="input-field col s12 m4 l4">                        <input type="text" name="rname<%-i%>" id="rname<%-i%>" class="materialize-textarea" value="<%-user.emergency[i]['name']%>">                        <label for="rname<%-i%>">Name of contact</label>                    </div>                     <div class="input-field col s12 m4 l4">                        <input id="rcontact<%-i%>" type="number" class="validate" name="rcontact<%-i%>" value="<%- user.emergency[i]['contact']%>" required>                        <label for="rcontact<%-i%>">Contact Number</label>                    </div>                    <div class="btn-floating btn-small waves-effect blue delBtn" onclick="delRow(<%-i%>)"><i class="material-icons">delete</i></div>                </div>            <%}%>        </span>        <input type="text" name="emergencyArray" id="emergencyArray" hidden>        <input type="text" name="allergiesArray" id="allergiesArray" hidden>        <div class="btn-floating btn-small waves-effect waves-light blue right" id="addNewRow"><i class="material-icons">add</i></div>        <div align="center">            <button type="submit" id="submittest" class="waves-effect waves-light btn blue z-depth-4">Confirm</button>            <button type="submit" id="submit" class="waves-effect waves-light btn blue z-depth-4 hide">Submit</button>        </div>    </form></div><%- include('../../layout/footer'); -%><script src="/js/emergency.js"></script>