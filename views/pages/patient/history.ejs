<%- include('../../layout/patient_layout'); -%><%let docArray={};for(i=0;i<doc.length;i++){    docArray[doc[i]['_id']]={'name':doc[i]['name'],'contact':doc[i]['contact'],'imageUrl':doc[i]['imageUrl']};}%><div class="container center-block">    <ul class="collapsible  popout">        <%for (d=0,len=data.length;d<=len;d++){ %>            <% for (i=0,len=data[d].prescription.length-1;i<=len;i++){ %>                <li>                    <div class="collapsible-header">                        <div class="row">                            <div class="col s2 m2 l2">                                <img src="<%-  docArray[data[d]['did']]['imageUrl']%>" alt=""  width="100%" >                            </div>                            <div class="col s10  m10 l10">                                <p>Doctor : <%- docArray[data[d]['did']]['name']%></p>                                <p><i class="material-icons left">phone</i>Contact : <%- docArray[data[d]['did']]['contact']%></p>                                <p>                                    <i class="material-icons left">access_time</i> Created At :                                    <%- moment(data[d]['prescription'][i]['updatedAt']).format('dddd, MMMM Do YYYY, h:mm')%>                                </p>                            </div>                        </div>                    </div>                    <div class="collapsible-body">                        <p>Findings :                            <% for(j=0,l= Object.keys(data[d]['prescription'][i]['findings']).length;j<l;j++){ %>                                <%- data[d]['prescription'][i]['findings'][j]['name']%>                            <%}%>                        </p>                        <p>Notes : <%- data[d]['prescription'][i]['notes']%></p>                        <table>                            <thead>                            <tr>                                <th>Medicine</th>                                <th>When</th>                                <th>Duration</th>                            </tr>                            </thead>                            <tbody>                            <% for(j=0,l= Object.keys(data[d]['prescription'][i]['medicine']).length;j<l;j++){ %>                                <tr>                                    <td>                                        <%- data[d]['prescription'][i]['medicine'][j]['name']%>                                    </td>                                    <td>                                        <%                                            let a = Object.keys(data[d]['prescription'][i]['medicine'][j]['when']);                                            let b = a.length;                                        for(k=0;k<b;k++){                                        %>                                            <div class="chip"><%- a[k]%></div>                                        <%}%>                                    </td>                                    <td>                                        <%- data[d]['prescription'][i]['medicine'][j]['duration']%>                                    </td>                                </tr>                            <%}%>                        </table>                        <div class="center-block center">                        </div>                    </div>                </li>            <%} %>        <%}%>    </ul></div><%- include('../../layout/footer'); -%>