<%- include('../../layout/patient_layout'); -%><%let today = new Date(); %><!--<%- JSON.stringify(data)%>--><%if(data.length <=0){%>    <main class="center" style="margin-top: 10%">        <i class="material-icons  large">error</i>        <h4 class="">You dont have any active medication</h4>    </main>    <div class="center">        <a href="" class="btn center center-block">View History</a>    </div><%}else{%>    <div class="container">        <table>            <thead>            <tr>                <th>Medicine</th>                <th>When</th>                <th>Duration</th>            </tr>            </thead>            <tbody>            <%for (i=0,len=data.length;i<len;i++) {            for (j=0,n=data[i]['prescription'].length;j<n;j++) {            for (k=0,m=Object.keys(data[i]['prescription'][j]['medicine']).length;k<m;k++){            if(moment(data[i]['prescription'][j]['medicine'][k]['duration']).format('YYYYMMDD')>=moment(today).format('YYYYMMDD')){            %>            <tr>                <td>                    <%- data[i]['prescription'][j]['medicine'][k]['name']%>                </td>                <td>                    <%                        let a = Object.keys(data[i]['prescription'][j]['medicine'][k]['when']);                        let b = a.length;                    for(k=0;k<b;k++){                    %>                        <div class="chip"><%- a[k]%></div>                    <%}%>                </td>                <td>                    <%- data[i]['prescription'][j]['medicine'][j]['duration']%>                </td>                <%}                }                }                }%>            </tbody>        </table>    </div><%}%><%- include('../../layout/footer'); -%>